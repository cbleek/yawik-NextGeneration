{
    "type": "project",
    "license": "MIT",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Yawik\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yawik\\Tests\\": "tests/",
            "spec\\Yawik\\": "spec/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "5.3.*"
        }
    },
    "require": {
        "php": ">=8.0.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-mongodb": "*",
        "api-platform/core": "^2.6",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.0",
        "doctrine/mongodb-odm-bundle": "^4.3",
        "guzzlehttp/guzzle": "^7.0",
        "nelmio/alice": "^3.8",
        "nelmio/cors-bundle": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "symfony/asset": "5.3.*",
        "symfony/console": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/expression-language": "5.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.3.*",
        "symfony/mercure-bundle": "^0.3.2",
        "symfony/monolog-bundle": "^3.5",
        "symfony/property-access": "5.3.*",
        "symfony/property-info": "5.3.*",
        "symfony/runtime": "5.3.*",
        "symfony/security-bundle": "5.3.*",
        "symfony/serializer": "5.3.*",
        "symfony/translation": "5.3.*",
        "symfony/twig-bundle": "5.3.*",
        "symfony/validator": "5.3.*",
        "symfony/yaml": "5.3.*",
        "theofidry/alice-data-fixtures": "^1.4"
    },
    "require-dev": {
        "api-platform/schema-generator": "^3.0",
        "behat/behat": "^3.8",
        "doctrine/data-fixtures": "^1.5",
        "dvdoug/behat-code-coverage": "^5.2",
        "friends-of-behat/mink-browserkit-driver": "^1.5",
        "friends-of-behat/mink-extension": "^2.5",
        "friends-of-behat/symfony-extension": "^2.2",
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpspec/phpspec": "^7.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.96",
        "phpstan/phpstan-doctrine": "^0.12.43",
        "phpstan/phpstan-phpunit": "^0.12.22",
        "phpstan/phpstan-symfony": "^0.12.42",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.16.1",
        "psalm/plugin-symfony": "^3.0",
        "soyuka/contexts": "^3.3",
        "symfony/browser-kit": "5.3.*",
        "symfony/css-selector": "5.3.*",
        "symfony/debug-bundle": "5.3.*",
        "symfony/maker-bundle": "^1.21",
        "symfony/phpunit-bridge": "5.3.*",
        "symfony/stopwatch": "5.3.*",
        "symfony/var-dumper": "5.3.*",
        "symfony/web-profiler-bundle": "5.3.*",
        "vimeo/psalm": "^4.9"
    }
}
